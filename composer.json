{
	"name": "innovative/laravel",
	"description": "Core compatible Laravel project for Laravel 4.2",
	"license": "None",
	"repositories": [
	    {
            "type": "vcs",
            "url": "git@github.com:innovative-media/TestDummy.git"
        },
	    {
	        "type": "composer",
	        "url": "http://packages.imdadmin.com"
	    }
	],
	"require": {
		"laravel/framework": "~4.2",
		"innovative/core": "dev-develop",
		"doctrine/dbal": "~2.3",
		"msurguy/honeypot": "~0.2"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "1.*",
		"xethron/migrations-generator": "dev-master",
		"mockery/mockery": "0.9.*@dev",
		"laracasts/testdummy": "~2.0",
		"phpunit/phpunit": "4.3.*",
		"behat/behat": "~3.0",
		"behat/mink": "*",
        "behat/mink-extension": "*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-selenium2-driver": "*"
	},
	"autoload": {
		"classmap": [
			"app/database/migrations",
			"app/database/seeds"
		],
		"psr-4": {
			"App\\": "app"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"pre-commit install",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
